

<form method="post" action="/passengers">
  <label for="first-name-field">First Name:</label>
  <input
  type="text"
  name="first_name"
  id="first-name-field"
  />

  <label for="last-name-field">Last Name:</label>
  <input
  type="text"
  name="last_name"
  id="last-name-field"
  />

  <label for="nationality">Enter Nationality:</label>
  <input
  type="text"
  name="nationality"
  id="nationality-field"
  />

  <label for="date_of_purchase">Enter Date of Purchase:</label>
  <input
  type="date"
  name="date_of_purchase"
  id="date_of_purchase-field"
  />

 <label for="arrival-selection">Has the Passenger Arrived?:</label>
 <select name="arrived" id="passenger-selection">
   <option value="ARRIVED"> ARRIVED </option>
   <option value="NOT ARRIVED"> NOT ARRIVED </option>
   </select>
 


  <input type="submit" value="Add Passenger">
</form>


